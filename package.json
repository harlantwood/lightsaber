{
  "name": "lightsaber",
  "version": "0.5.1",
  "description": "Lightweight jedi utility toolbelt, written in coffeescript",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./lib/",
    "compile": "./node_modules/.bin/coffee --output lib/ --compile src/",
    "prepublish": "npm run compile",
    "test": "node_modules/.bin/mocha --recursive --compilers coffee:coffee-script/register spec",
    "watch": "./node_modules/.bin/coffee --watch --output lib/ --compile src/"
  },
  "author": "Harlan T Wood",
  "license": "ISC",
  "dependencies": {
    "chalk": "^1.1.1",
    "jssha": "^1.5.0",
    "lodash": "^4.5.0",
    "shelljs": "^0.5.3"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "coffee-script": "^1.8.0",
    "mocha": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harlantwood/lightsaber.git"
  },
  "keywords": [
    "log",
    "typeof",
    "collection",
    "sha384",
    "sha512"
  ],
  "bugs": {
    "url": "https://github.com/harlantwood/lightsaber/issues"
  }
}
